<div>
  <% @tweets.each do |tweet| %>
    <div class="row">
      <div class="col s12 m6">
        <div class="card blue-grey darken-1">
          <div class="card-content white-text">
            <%= simple_format(tweet.text) %>
          </div>
          <div class="card-action">
            <a href="#">EDIT</a>
            <a href="#">DELETE</a>
        </div>
        </div>
      </div>
    </div>
  <% end %>
  <%= paginate(@tweet) %>

<script type="text/javascript" src="./app.js"></script>
</div>
